[time]
command=date +%H:%M
interval=60
label=

[date]
command=date "+%b %d"
interval=60
label=

[keyboard]
command=kb-layout print %s
interval=once
label=
signal=1

[battery]
command=echo $(acpi | egrep -o "[0-9]+%")
interval=30
label=

[memory]
command=free -h | awk '/^Mem/ {printf "%d%%",100*$3/$2}'
interval=5
label=
